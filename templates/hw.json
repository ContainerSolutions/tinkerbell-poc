{
  "id": "${UUID}",
  "metadata": {
    "facility": {
      "facility_code": "onprem initrd=initramfs-aarch64 #"
    },
    "instance": {},
    "state": ""
  },
  "network": {
    "interfaces": [
      {
        "dhcp": {
          "arch": "aarch64",
          "ip": {
            "address": "${IP}",
            "gateway": "{GW}",
            "netmask": "{MASK}"
          },
          "mac": "${MAC}",
          "hostname": "${HOSTNAME}",
          "iface_name": "eth0",
          "uefi": false
        },
        "netboot": {
          "allow_pxe": true,
          "allow_workflow": true
        }
      }
    ]
  }
}
