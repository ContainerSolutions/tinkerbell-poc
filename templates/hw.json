{
  "id": "${UUID}",
  "arch": "aarch64",
  "allow_pxe": true,
  "hostname": "${HOSTNAME}",
  "facility_code": "onprem initrd=initramfs-aarch64 #",
  "ip_addresses": [
    {
      "address": "${IP}",
      "netmask": "${MASK}",
      "gateway": "${GW}",
      "address_family": 4
    }
  ],
  "network_ports": [
    {
      "data": {
        "mac": "${MAC}"
      },
      "name": "eth0",
      "type": "data"
    }
  ],
  "allow_workflow": true
}
